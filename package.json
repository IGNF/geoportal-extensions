{
  "name": "geoportal-extensions",
  "leafletExtName": "French Geoportal Extension for Leaflet",
  "ol3ExtName": "French Geoportal Extension for OpenLayers 3",
  "itownsExtName": "French Geoportal Extension for Itowns",
  "version": "0.1.0",
  "leafletExtVersion": "1.0.0",
  "ol3ExtVersion": "1.0.0",
  "itownsExtVersion": "2.3.0",
  "description": "French Geoportal Extensions for OpenLayers, Leaflet and iTowns libraries",
  "main": "dist/leaflet/GpPluginLeaflet.js, dist/ol3/GpPluginOl3.js, dist/itowns/GpPluginItowns.js,",
  "directories": {
    "doc": "doc",
    "test": "test",
    "lib": "lib",
    "example": "samples"
  },
  "scripts": {
    "build": "npm run build-mix-itowns-ol3 && npm run build-leaflet",
    "build-ol3": "gulp build",
    "build-leaflet": "gulp build --leaflet",
    "build-itowns": "gulp build --itowns",
    "build-mix-itowns-ol3": "gulp build --mix --itowns --ol3",
    "build-prod": "npm run build-mix-itowns-ol3:prod && npm run build-leaflet:prod",
    "build-ol3:prod": "gulp build --production",
    "build-leaflet:prod": "gulp build --production --leaflet",
    "build-itowns:prod": "gulp build --production --itowns",
    "build-mix-itowns-ol3:prod": "gulp build --production --mix --itowns --ol3",
    "publish-all": "gulp publish --ol3 && gulp publish --leaflet && gulp publish --itowns && gulp publish --ol3 --itowns --mix",
    "test": "gulp test",
    "doc": "gulp doc",
    "check": "gulp check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IGNF/geoportal-extensions.git"
  },
  "author": "IGNF",
  "keywords": [
    "geoportail",
    "javascript",
    "OpenLayers",
    "Leaflet",
    "Itowns",
    "3D"
  ],
  "license": "CECILL-B",
  "bugs": {
    "url": "https://github.com/IGNF/geoportal-extensions/issues"
  },
  "homepage": "https://github.com/IGNF/geoportal-extensions#readme",
  "dependencies": {
    "geoportal-access-lib": "^1.2.0",
    "leaflet": "^1.3.0",
    "leaflet-draw": "0.4.12",
    "openlayers": "4.0.1",
    "itowns": "2.3.0",
    "proj4": "2.4.4",
    "proj4leaflet": "1.0.2",
    "requirejs": "^2.3.5",
    "sortablejs": "1.4.0"
  },
  "devDependencies": {
    "amdclean": "^2.7.0",
    "chai": "^3.4.1",
    "easy-amdtest": "^0.1.0",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.1",
    "gulp-clean-css": "^3.9.0",
    "gulp-cli": "^1.4.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^3.1.0",
    "gulp-hb": "^6.0.2",
    "gulp-header": "^1.7.1",
    "gulp-jscs": "~3.0.0",
    "gulp-jshint": "~1.5.0",
    "gulp-load-plugins": "^1.1.0",
    "gulp-mocha-phantomjs": "^0.12.1",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-size": "^2.0.0",
    "gulp-strip-comments": "^2.2.1",
    "gulp-template": "^3.1.0",
    "gulp-umd": "^0.2.0",
    "gulp-util": "^3.0.7",
    "handlebars": "^4.0.10",
    "handlebars-helpers": "^0.9.8",
    "handlebars-layouts": "^3.1.4",
    "jsdoc": "^3.5.5",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "open": "0.0.5",
    "rimraf": "^2.4.4",
    "run-sequence": "^1.1.5",
    "shelljs": "^0.5.3",
    "should": "^7.1.1",
    "sinon": "^1.17.2",
    "woodman": "^1.1.0"
  }
}
